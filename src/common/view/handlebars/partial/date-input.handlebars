<input  class='input-group__input date-input'
        {{#if id}}          id='{{id}}' {{/if}}
        {{#if attribute}}   name='{{attribute}}' {{/if}}
        {{#if value}}       value='{{value}}' {{/if}}
        {{#if required}}    required {{/if}}
        {{#if disabled}}    disabled {{/if}}
        {{#if readOnly}}    readonly {{/if}}
        {{#if max}}         max='{{max}}' {{/if}}
        {{#if min}}         min='{{min}}' {{/if}}
        {{#if title}}       title='{{title}}' {{/if}}
        autocomplete='{{#if autocomplete}}on{{/if}}{{#unless autocomplete}}off{{/unless}}'
        type='date'>
